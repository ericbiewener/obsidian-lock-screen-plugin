	"devDependencies": {
		"@types/fs-extra": "11.0.1",
		"@types/inquirer": "9.0.3",
		"@types/node": "20.4.8",
		"@typescript-eslint/eslint-plugin": "6.2.1",
		"@typescript-eslint/parser": "6.2.1",
		"builtin-modules": "3.3.0",
		"compare-versions": "6.0.0",
		"esbuild": "0.18.18",
		"eslint": "8.46.0",
		"execa": "5.1.1",
		"fs-extra": "11.1.1",
		"globby": "11.1.0",
		"husky": "^8.0.3",
		"inquirer": "8.2.6",
		"lint-staged": "^13.2.3",
		"obsidian": "1.4.0",
